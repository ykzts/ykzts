{
  "$schema": "https://openapi.vercel.sh/microfrontends.json",
  "applications": {
    "@ykzts/admin": {
      "development": {
        "fallback": "ykzts-admin.vercel.app",
        "local": 3001
      },
      "routing": [
        {
          "group": "admin",
          "paths": ["/admin", "/admin/:path*"]
        }
      ]
    },
    "@ykzts/portfolio": {
      "development": {
        "fallback": "ykzts.vercel.app",
        "local": 3000
      }
    }
  }
}
