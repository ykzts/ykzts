{
  "$schema": "https://openapi.vercel.sh/microfrontends.json",
  "applications": {
    "admin": {
      "development": {
        "fallback": "ykzts-admin.vercel.app",
        "local": 3001
      },
      "packageName": "@ykzts/admin",
      "routing": [
        {
          "group": "admin",
          "paths": ["/admin", "/admin/:path*"]
        }
      ]
    },
    "blog": {
      "development": {
        "fallback": "ykzts.com",
        "local": 3002
      },
      "packageName": "@ykzts/blog",
      "routing": [
        {
          "group": "blog",
          "paths": ["/blog", "/blog/:path*"]
        }
      ]
    },
    "portfolio": {
      "development": {
        "fallback": "ykzts.com",
        "local": 3000
      },
      "packageName": "@ykzts/portfolio"
    }
  }
}
