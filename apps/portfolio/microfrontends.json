{
  "$schema": "https://openapi.vercel.sh/microfrontends.json",
  "applications": {
    "blog": {
      "development": {
        "fallback": "ykzts.com",
        "local": 3002
      },
      "packageName": "@ykzts/blog",
      "routing": [
        {
          "group": "blog",
          "paths": ["/blog", "/blog/:path*", "/api/blog/:path*"]
        }
      ]
    },
    "portfolio": {
      "development": {
        "fallback": "ykzts.com",
        "local": 3000
      },
      "packageName": "@ykzts/portfolio"
    }
  }
}
